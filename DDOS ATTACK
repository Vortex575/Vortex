ip =input(Fore.GREEN+'Enter your IP =>>')

while True:
    sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    sock.settimeout(1)  # ØªØ¹ÙŠÙŠÙ† Ù‚ÙŠÙ…Ø© settimeout() Ø¹Ù„Ù‰ Ù…ØªØºÙŠØ± sock
